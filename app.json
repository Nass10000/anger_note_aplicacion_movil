{
  "expo": {
    "name": "Registro de Enojo",
    "slug": "angertrack-simple",
    "version": "1.0.0",
    "scheme": "angertrack",
    "platforms": [
      "android"
    ],
    "android": {
      "package": "com.nass.angertrack",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#8d1a1a"
      }
    },
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#2a0a0a"
    },
    "updates": {
      "enabled": true
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "extra": {
      "eas": {
        "projectId": "3e781bd2-7679-44c6-964c-9c8e77b5011c"
      }
    },
    "plugins": [
      "expo-secure-store"
    ]
  }
}
